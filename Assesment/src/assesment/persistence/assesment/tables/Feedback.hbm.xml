<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.assesment.tables" >

	<class name="Feedback" table="feedbacks">
       <composite-id class="FeedbackPK" name="pk" >
       		<key-many-to-one name="assesment" column="assesment" class="Assesment" />
	        <key-property	name="email"	type="java.lang.String"	column="email"/>
	        <key-property	name="type"	type="java.lang.Integer"	column="type"/>
       </composite-id>
  	</class>

</hibernate-mapping>
