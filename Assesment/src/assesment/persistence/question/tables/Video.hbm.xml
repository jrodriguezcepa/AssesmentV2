<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.question.tables" >

	<class name="Video" table="videos">
		<id	column="id"	name="id" type="java.lang.Integer">
            <generator class="sequence">
            	<param name="sequence">Video_Sequence</param>
            </generator>
		</id>
	    
	    <property name="key" column="key" type="java.lang.String" not-null="true"/>
	    <property name="language" column="language" type="java.lang.String" not-null="true"/>
	    
  	</class>

</hibernate-mapping>
