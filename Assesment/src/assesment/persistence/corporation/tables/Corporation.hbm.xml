<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.corporation.tables" >

	<class name="Corporation" table="corporations">
		<id	column="id"	name="id" type="java.lang.Integer">
            <generator class="sequence">
            	<param name="sequence">Corporation_Sequence</param>
            </generator>
		</id>
	    
	    <property name="name" column="name" type="java.lang.String" not-null="true"/>
	    <property name="logo" column="logo" type="java.lang.String"/>
	    
		<set inverse="true" name="AssesmentSet"  cascade="all-delete-orphan">
			<key column="corporation" not-null="true"/>
			<one-to-many class="assesment.persistence.assesment.tables.Assesment" />
		</set>
		
		<property name="dc5id" column="dc5id" type="java.lang.Integer" />

  	</class>

</hibernate-mapping>
