<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.user.tables">
    <class name="UserLogin" table="userslogins">

       <composite-id class="UserLoginPK" name="pk" >
	        <key-property	name="loginName"	type="java.lang.String"	column="loginname"/>
	        <key-property	name="loginDate"	type="java.lang.String"	column="logindate"/>
       </composite-id>
    </class>

</hibernate-mapping>
