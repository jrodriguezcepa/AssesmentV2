<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.user.tables" >

	<class name="SendedReport" table="sendedreports">
		<id	column="id"	name="id" type="java.lang.Integer">
            <generator class="sequence">
            	<param name="sequence">SendedReport_Sequence</param>
            </generator>
		</id>
	    
	    <property name="login" 			column="login" 			type="java.lang.String" not-null="true"/>
        <property name="assessment"		column="assessment" 	type="java.lang.Integer" not-null="true"/>

        <property name="sended" type="java.lang.Boolean" not-null="true" >
			<column name="sended" default="false" />
		</property>
        <property name="certificate" type="java.lang.Boolean" not-null="true" >
			<column name="certificate" default="false" />
		</property>
   	</class>

</hibernate-mapping>
