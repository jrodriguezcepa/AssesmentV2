<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.language.tables">
    <class
        name="Language"
        table="languages"
    >

        <id
            name="name"
            column="name"
            type="java.lang.String"
            length="50"
        >
            <generator class="assigned">
            </generator>
        </id>
    </class>

</hibernate-mapping>
