<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.tag.tables" >

	<class name="AnswerTag" table="answertags">
       <composite-id class="AnswerTagPK" name="pk" >
       		<key-many-to-one
       			 name="answer"
       			 column="answer"
       			 class="assesment.persistence.question.tables.Answer"
       	 	/>
       	 	<key-many-to-one
       	 		 name="tag"
       	 		 column="tag"
       	 		 class="assesment.persistence.tag.tables.Tag"
       	 	/>
       </composite-id>

	    <property name="value" column="value" type="java.lang.Integer" not-null="true"/>
	    
  	</class>

</hibernate-mapping>
