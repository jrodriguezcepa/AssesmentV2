<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.tag.tables" >

	<class name="AssesmentTag" table="assesmenttags">
       <composite-id class="AssesmentTagPK" name="pk" >
       		<key-many-to-one
       			 name="assesment"
       			 column="assesment"
       			 class="assesment.persistence.assesment.tables.Assesment"
       	 	/>
       	 	<key-many-to-one
       	 		 name="tag"
       	 		 column="tag"
       	 		 class="assesment.persistence.tag.tables.Tag"
       	 	/>
       </composite-id>

	    <property name="min" column="min" type="java.lang.Integer" not-null="true"/>
	    
  	</class>

</hibernate-mapping>
