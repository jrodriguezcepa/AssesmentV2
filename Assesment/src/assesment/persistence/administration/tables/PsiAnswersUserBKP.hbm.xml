<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.administration.tables" >
    <class name="PsiAnswersUserBKP" table="userpsianswersbkp">

        <composite-id>
      	<key-property 	name="assesment"		type="java.lang.Integer"	column="assesment" />
        <key-property 	name="psianswer"		type="java.lang.Integer"	column="psianswer" />
        <key-property 	name="loginName"			type="java.lang.String"	column="loginName" />
        </composite-id>
       
	</class>

</hibernate-mapping>
