<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.administration.tables" >

	<class name="FailedAssessment" table="failedassessments">
		<id	column="id"	name="id" type="java.lang.Integer">
            <generator class="sequence">
            	<param name="sequence">FailedAssessment_Sequence</param>
            </generator>
		</id>
	    
	    <property name="endDate" column="enddate" type="java.util.Date" not-null="true"/>

        <property name="loginName"	 type="java.lang.String"	column="loginname" />

        <property name="assessment"	 type="java.lang.Integer"	column="assessment" />
  	</class>

</hibernate-mapping>
