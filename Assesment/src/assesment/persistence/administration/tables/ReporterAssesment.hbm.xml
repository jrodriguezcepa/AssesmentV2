<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="assesment.persistence.administration.tables" >
    <class name="ReporterAssesment" table="reporterassesments">

       <composite-id class="UserAssesmentPK" name="pk" >
       		<key-many-to-one
       			 name="assesment"
       			 column="assesment"
       			 class="assesment.persistence.assesment.tables.Assesment"
       	 	/>
       	 	<key-many-to-one
       	 		 name="user"
       	 		 column="loginname"
       	 		 class="assesment.persistence.user.tables.User"
       	 	/>
       </composite-id>
	</class>

</hibernate-mapping>
